<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="AdBoxBundle\Entity\Pub" table="pub">
    <indexes>
      <index name="id_intervalle" columns="id_timelaps"/>
      <index name="id_media" columns="id_media"/>
      <index name="id_shop" columns="id_shop"/>
      <index name="id_timelaps" columns="id_timelaps"/>
      <index name="id_media_2" columns="id_media"/>
    </indexes>
    <id name="id" type="integer" column="id">
      <generator strategy="IDENTITY"/>
    </id>
    <many-to-one field="idShop" target-entity="Shop">
      <join-columns>
        <join-column name="id_shop" referenced-column-name="id"/>
      </join-columns>
    </many-to-one>
    <many-to-one field="idTimelaps" target-entity="Timelaps">
      <join-columns>
        <join-column name="id_timelaps" referenced-column-name="id"/>
      </join-columns>
    </many-to-one>
    <many-to-one field="idMedia" target-entity="Media">
      <join-columns>
        <join-column name="id_media" referenced-column-name="id"/>
      </join-columns>
    </many-to-one>
  </entity>
</doctrine-mapping>
