<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="AdBoxBundle\Entity\Shop" table="shop">
    <unique-constraints>
      <unique-constraint name="id_adress" columns="id_adress"/>
    </unique-constraints>
    <id name="id" association-key="true"/>
    <field name="name" type="string" column="name" length="100" nullable="false"/>
    <field name="ownerId" type="integer" column="owner_id" nullable="false"/>
    <one-to-one field="id" target-entity="ShopOwner">
      <join-columns>
        <join-column name="id" referenced-column-name="id"/>
      </join-columns>
    </one-to-one>
    <many-to-one field="idAdress" target-entity="Adresse">
      <join-columns>
        <join-column name="id_adress" referenced-column-name="id"/>
      </join-columns>
    </many-to-one>
  </entity>
</doctrine-mapping>
