<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="AdBoxBundle\Entity\Timelaps" table="timelaps">
    <indexes>
      <index name="id_pointpub" columns="id_pointpub"/>
    </indexes>
    <id name="id" type="integer" column="id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="priorite" type="integer" column="priorite" nullable="false"/>
    <field name="etat" type="boolean" column="etat" nullable="false"/>
    <field name="prix" type="float" column="prix" precision="10" scale="0" nullable="false"/>
    <field name="dateStart" type="datetime" column="date_start" nullable="false"/>
    <field name="dateEnd" type="datetime" column="date_end" nullable="false"/>
    <many-to-one field="idPointpub" target-entity="Shop">
      <join-columns>
        <join-column name="id_pointpub" referenced-column-name="id"/>
      </join-columns>
    </many-to-one>
  </entity>
</doctrine-mapping>
