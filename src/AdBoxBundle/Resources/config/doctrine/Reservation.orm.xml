<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="AdBoxBundle\Entity\Reservation" table="reservation">
    <indexes>
      <index name="id_client" columns="id_client"/>
    </indexes>
    <id name="id" association-key="true"/>
    <one-to-one field="id" target-entity="Timelaps">
      <join-columns>
        <join-column name="id" referenced-column-name="id"/>
      </join-columns>
    </one-to-one>
    <many-to-one field="idClient" target-entity="Client">
      <join-columns>
        <join-column name="id_client" referenced-column-name="id"/>
      </join-columns>
    </many-to-one>
  </entity>
</doctrine-mapping>
