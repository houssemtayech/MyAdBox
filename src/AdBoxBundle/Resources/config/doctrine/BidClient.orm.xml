<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="AdBoxBundle\Entity\BidClient" table="bid_client">
    <indexes>
      <index name="id_bid" columns="id_bid"/>
      <index name="id_user" columns="id_client"/>
    </indexes>
    <id name="id" type="integer" column="id">
      <generator strategy="IDENTITY"/>
    </id>
    <many-to-one field="idClient" target-entity="Client">
      <join-columns>
        <join-column name="id_client" referenced-column-name="id"/>
      </join-columns>
    </many-to-one>
    <many-to-one field="idBid" target-entity="Bid">
      <join-columns>
        <join-column name="id_bid" referenced-column-name="id"/>
      </join-columns>
    </many-to-one>
  </entity>
</doctrine-mapping>
