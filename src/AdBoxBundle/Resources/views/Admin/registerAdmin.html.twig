  <!-- Page -->
      <div>
            {% if is_granted("IS_AUTHENTICATED_REMEMBERED") %}
                {{ 'layout.logged_in_as'|trans({'%username%': app.user.username}, 'FOSUserBundle') }} |
                <a href="{{ path('fos_user_security_logout') }}">
                    {{ 'layout.logout'|trans({}, 'FOSUserBundle') }}
                </a>
            {% else %}
                <a href="{{ path('fos_user_security_login') }}">{{ 'layout.login'|trans({}, 'FOSUserBundle') }}</a>
            {% endif %}
        </div>
  <div class="page animsition" data-animsition-in="fade-in" data-animsition-out="fade-out">
    <div class="page-content">
      <div class="page-brand-info">
        <div class="brand">
          <img class="brand-img" src="../assets/images/logo%402x.png" alt="...">
          <h2 class="brand-text font-size-40"></h2>
        </div>
       
      </div>

      <div class="page-register-main">
        <div class="brand visible-xs">
          <img class="brand-img" src="../assets/images/logo%402x.png" alt="...">
          <h3 class="brand-text font-size-40"></h3>
        </div>
        <h3 class="font-size-24">Sign Up Admin</h3>
    
{% trans_default_domain 'FOSUserBundle' %}

<form action="{{ path('user_admin_registration') }}" {{ form_enctype(form) }} method="POST" class="fos_user_registration_register">
      <div class="form-group" id="fos_user_registration_form">
       
          <div class="form-group form-material floating">
            {{ form_widget(form.email, { 'attr': {'class': 'form-control'|trans } }) }}
        {{ form_errors(form.email) }}
            <label class="floating-label" for="inputEmail">Email</label>
          </div>
         
         <div class="form-group form-material floating">
           {{ form_widget(form.username, { 'attr': {'class': 'form-control'|trans } }) }}
        {{ form_errors(form.username) }}
            <label class="floating-label" for="inputName">Name</label>
          </div>
        
         <div class="form-group form-material floating">
            {{ form_widget(form.plainPassword.first, { 'attr': {'class': 'form-control'|trans } }) }}
        {{ form_errors(form.plainPassword.first) }}
            <label class="floating-label" for="inputPassword">Password</label>
          </div>
       
             <div class="form-group form-material floating">
         {{ form_widget(form.plainPassword.second, { 'attr': {'class': 'form-control'|trans } }) }}
        {{ form_errors(form.plainPassword.second) }}

            <label class="floating-label" for="inputPasswordCheck">Retype Password</label>
          </div>
       
           <div class="form-group clearfix">
            <div class="checkbox-custom checkbox-inline checkbox-primary pull-left">
             {{ form_rest(form) }}
              <label for="inputCheckbox"></label>
            </div>
            <p class="margin-left-35">By clicking Sign Up, you agree to our <a href="#">Terms</a>.</p>
          </div>
       
    </div>
    <div>
        <input type="submit" class="btn btn-primary btn-block" value="{{ 'Sign Up'|trans }}" />
    </div>
</form>

       
         
          {#
        
         
       
          <button type="submit" class="btn btn-primary btn-block">Sign Up</button>
        </form>#}

        <p>Have account already? Please go to <a href="{{path('login_page')}}">Sign In</a></p>

